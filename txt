		RootJoint.C0 = Clerp(RootJoint.C0, ROOTC0 * CF(0, 0, 0) * ANGLES(RAD(MRANDOM(-53,53)), RAD(MRANDOM(-64,64)), RAD(MRANDOM(-64,64))), 0.2 / Animation_Speed)
		Neck.C0 = Clerp(Neck.C0, NECKC0 * CF(0, 0, 0 + ((5.2) - 5)) * ANGLES(RAD(0 + MRANDOM(-150,150) - 4 * COS(SINE / 12)), RAD(MRANDOM(-250,250)), RAD(0)), 1.5 / Animation_Speed)

		RightShoulder.C0 = Clerp(RightShoulder.C0, CF(1.5 * Player_Size, 0.5 * Player_Size, 0 * Player_Size) * ANGLES(RAD(0), RAD(0), RAD(0)) * RIGHTSHOULDERC0, 0.15 / Animation_Speed)
